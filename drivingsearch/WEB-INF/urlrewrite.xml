<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.5//EN" "http://tuckey.org/res/dtds/urlrewrite2.5.dtd">
<urlrewrite>
    <rule>
        <from>^/(.*).html$</from>
        <to>/$1.jsp</to>
    </rule>
</urlrewrite>
<!-- ^/(.*)/(.*).html$ | ^(/.*\..html)$ -->